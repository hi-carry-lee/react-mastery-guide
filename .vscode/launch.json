{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug 2-react-ts",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}/projects/2-react-ts/src",
      "sourceMapPathOverrides": {
        "/@fs/*": "${workspaceFolder}/*",
        "/src/*": "${webRoot}/*"
      },
      "skipFiles": [
        "<node_internals>/**",
        "**/node_modules/**",
        "**/vite/**",
        "**/react/**",
        "**/react-dom/**",
        "**/react-refresh/**",
        "**/@vitejs/**",
        "**/esbuild/**",
        "**/rollup/**",
        "**/webpack/**",
        "**/chunk-*.js",
        "**/vendor-*.js",
        "**/*.hot-update.js",
        "**/hot-update.js",
        "**/@react-refresh/**",
        "**/react-refresh-runtime/**",
        "**/react-refresh-dom/**"
      ],
      "smartStep": true,
      "preLaunchTask": "start-2-react-ts-dev"
    }
  ]
}
